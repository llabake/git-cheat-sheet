  "name": "git-cheat-sheet",
  "version": "1.0.0",
  "description": "Git command cheat sheet built with MongoDB, ExpressJS, React and NodeJS.",
  "main": "index.js",
  "repository": "https://github.com/llabake/git-cheat-sheet.git",
  "author": "llabake <lemboyelabake@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-display-name": "2.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jsdom": "8.5.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "moxios": "^0.4.0",
    "nock": "8.0.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-hot-loader": "^4.3.11",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "2.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-client": "^4.1.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "6.8.0",
    "cors": "^2.8.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "express": "4.13.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.4.0",
    "nyc": "^13.1.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router": "2.4.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --colors --open",
    "start": "babel-node app.js",
    "build": "webpack --config webpack.config.prod.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint  \"client/**/*.{js,jsx}\" --quiet",
    "test:server": "export NODE_ENV=test && nyc --reporter=html --reporter=text mocha server/tests/*.test.js --timeout 10000 --exit --compilers js:@babel/register",
    "test:client": "jest -u --coverage"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/client/tests/jestSetup.js",
    "testPathIgnorePatterns": [
      "server"
    ],
    "coverageThreshold": {
      "rootDir": "./",
      "verbose": true,
      "collectCoverage": true,
      "collectCoverageFrom": [
        "./**/*.js",
        "!server/"
      ],
      "coverageDirectory": "<rootDir>/coverage",
      "moduleFileExtensions": [
        "js"
      ],
      "global": {
        "branches": 70,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|gif|ttf|woff|woff2)$": "identity-obj-proxy",
      "\\.(css|scss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
